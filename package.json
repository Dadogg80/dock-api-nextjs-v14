{
  "name": "dock-api",
  "version": "2.0.0",
  "description": "Dock Issuer API, built in an NextJS v.14 app",
  "scripts": {
    "all": "for FILE in ./workflows/*.js; do node $FILE; done",
    "anchors": "node ./workflows/anchorsFlow.js",
    "dids": "node ./workflows/didFlow.js",
    "schemas": "node ./workflows/schemaFlow.js",
    "registries": "node ./workflows/registryFlow.ts",
    "credentials": "node ./workflows/credentialsFlow.js",
    "presentations": "node ./workflows/presentationsFlow.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "private": true,
  "dependencies": {
    "react": "^18",
    "react-dom": "^18",
    "next": "14.1.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0"
  }
}
